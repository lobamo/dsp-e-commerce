<h1>Homepage#index</h1>
<div class="container">
    <div class="row">
     <% @products.first(10).each do |product| %>
        <div class="col-md-6">
            <%= product.title%>
            <%= image_tag(product.image)%>
            <%= product.description%>
        </div>
        <% end %>
    </div>
</div>
